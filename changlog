document.getElementById("word").addEventListener("click", hideExcel);
document.getElementById("excel").addEventListener("click", hideWord);

function hideExcel() {
    $('.excel').hide();
    $('.word').show();
    $('#wordTab').css('background-color', '#0072c6');
    $('#wordTab a').css('color', '#fff');
    $('#excelTab').css('background-color', '#E4E4E4');
    $('#excelTab a').css('color', '#0072c6');
}

function hideWord() {
    $('.word').hide();
    $('.excel').show();
    $('#wordTab').css('background-color', '#E4E4E4');
    $('#wordTab a').css('color', '#0072c6');
    $('#excelTab').css('background-color', '#0072c6');
    $('#excelTab a').css('color', '#fff');
}
